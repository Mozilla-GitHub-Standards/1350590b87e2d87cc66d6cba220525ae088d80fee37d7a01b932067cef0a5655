{
	"queue":{
		"type":"mongodb",
		"host":"localhost",
		"port":"27017"
	},
	"pulse":{
		"applabel": "catalog-listener-dev",
		"topic": "unittest.#",
		"durable":true,
		"$ref":"file://~/.catalogrc#pulse"
	},
	"datastore":{
		"type":"elasticsearch",
		"host":"http://localhost",
		"port":"9200",
		"index":"unittest"
	},
	"work_queues": ["rq", "mongo", "simple"],
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"From": "https://github.com/mozilla/structured-catalog"
		}
	}
}
